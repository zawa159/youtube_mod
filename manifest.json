{
  "manifest_version": 3,
  "name": "Youtube_mod_develop",
  "author": "zawa159",
  "version": "1.0.1",
  "description": "Youtubeの拡張機能",
  "content_scripts": [{
    "matches": ["*://*.youtube.com/*"],
    "run_at": "document_end",
    "all_frames": true,
    "js": ["youtube_mod_main.js"]
  }],
  "action": {
    "default_title":"Youtube_mod",
    "default_popup":"popup.html"
  }
  
}